<view hidden="{{!(showSite)}}" class="popup site"><view class="mask"></view><view class="layer"><view class="site-header"><text class="font">配送至</text><text data-event-opts="{{[['tap',[['hideSpec',['$event']]]]]}}" class="icon iconfont" catchtap="__e"></text></view><view class="nav"><block tt:for="{{siteNav}}" tt:for-item="name" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['showSiteNav',[index]]]]]}}" class="{{(('type')+' '+((index==showSiteIndex)?'cur':''))}}" bindtap="__e">{{name}}</view></block></view><scroll-view class="city" scroll-y="true"><block tt:for="{{province}}" tt:for-item="prov" tt:for-index="index1" tt:key="index1"><block tt:if="{{showOne}}"><view data-event-opts="{{[['tap',[['showProvNav',['$0',index1],[[['province','',index1]]]]]]]}}" class="{{(('row')+' '+((index1==showProvIndex)?'on':''))}}" bindtap="__e">{{''+prov+''}}</view></block></block><block tt:for="{{citys}}" tt:for-item="city" tt:for-index="index2" tt:key="index2"><block tt:if="{{showTwo}}"><view data-event-opts="{{[['tap',[['showCityNav',['$0',index2],[[['citys','',index2]]]]]]]}}" class="{{(('row')+' '+((index2==showCityIndex)?'on':''))}}" bindtap="__e">{{''+city+''}}</view></block></block><block tt:for="{{region}}" tt:for-item="reg" tt:for-index="index3" tt:key="index3"><block tt:if="{{showThree}}"><view data-event-opts="{{[['tap',[['overSelect',['$0',index3],[[['region','',index3]]]]]]]}}" class="{{(('row')+' '+((index3==showRegIndex)?'on':''))}}" bindtap="__e">{{''+reg+''}}</view></block></block></scroll-view></view></view>