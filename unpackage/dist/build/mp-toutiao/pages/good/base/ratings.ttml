<view><block tt:if="{{showNull}}"><view class="null">暂无评论~</view></block><view class="content"><view class="list"><block tt:for="{{ratingsList}}" tt:for-item="row" tt:for-index="Rindex" tt:key="Rindex"><view class="row"><view class="left"><view class="face"><image src="{{row.face||'https://ss1.bdstatic.com/70cFuXSh_Q1YnxGkpoWK1HF6hhy/it/u=145692761,4091651670&fm=26&gp=0.jpg'}}"></image></view></view><view class="right"><view class="name-date"><view class="username">{{''+(row.username||'砍砍用户')+''}}</view><view class="star"><block tt:for="{{clicked_list}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="iconfont"><block tt:if="{{item}}"><view></view></block><block tt:else><view class="starIcon"></view></block></view></block></view></view><view class="spec"><view class="date">{{''+row.time+''}}</view><view>{{''+(row.spec||'')+''}}</view></view><view class="first"><view class="rat">{{''+row.content+''}}</view><view class="img-video"><block tt:for="{{row.video}}" tt:for-item="item" tt:for-index="__i0__" tt:key="path"><view data-event-opts="{{[['tap',[['playVideo',['$0'],[[['ratingsList','',Rindex],['video','path',item.path,'path']]]]]]]}}" class="box" bindtap="__e"><image mode="aspectFill" src="{{item.img}}"></image><view class="playbtn"><view class="icon iconfont"></view></view></view></block><block tt:for="{{row.img}}" tt:for-item="item" tt:for-index="__i1__" tt:key="*this"><view data-event-opts="{{[['tap',[['showBigImg',['$0','$1'],[[['ratingsList','',Rindex],['img','',__i1__]],[['ratingsList','',Rindex,'img']]]]]]]}}" class="box" bindtap="__e"><image mode="aspectFill" src="{{item}}"></image></view></block></view></view></view></view></block></view></view></view>