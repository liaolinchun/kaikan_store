<view><block tt:for="{{goodsInfo}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="box"><view class="time">{{"结束时间 "+item.end_time}}</view><view class="main"><view class="left"><image src="{{'http://www.tiktokshopee.com'+item.thumbimg}}"></image></view><view class="center"><view class="top">{{item.title}}</view><view class="btn"><text class="new">{{"￥"+item.on_price}}</text><text class="old">{{"￥"+item.price_one}}</text></view></view><view class="right"><view>{{item.end}}</view></view></view><view class="footer"><view></view><block tt:if="{{item.end!='游戏结束'}}"><view class="btn"><view data-event-opts="{{[['tap',[['share',['$0'],[[['goodsInfo','',index]]]]]]]}}" class="type" bindtap="__e">分享帮砍</view><view data-event-opts="{{[['tap',[['toOrder',['$0'],[[['goodsInfo','',index]]]]]]]}}" class="type cur" bindtap="__e">去下单</view></view></block><block tt:else><view class="btn"><view data-event-opts="{{[['tap',[['delGame',['$0'],[[['goodsInfo','',index,'id']]]]]]]}}" class="type cur" bindtap="__e">删除商品</view></view></block></view></view></block><block tt:if="{{wechatShow}}"><wechat class="vue-ref" vue-id="27b89728-1" goodsInfo="{{goodsInfoCur}}" kanId="{{kanId}}" child="{{child}}" money="{{money}}" data-ref="Series" bind:__l="__l"></wechat></block><block tt:if="{{loading}}"><view class="loading-text">{{loadingText}}</view></block></view>