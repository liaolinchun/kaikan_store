page{background-color:#f2f2f2}.main{width:92%;padding:20rpx 4%;background-color:#fff;margin-top:5rpx}.main .time{padding:5rpx 0;font-size:24rpx;color:#999}.main .info{width:100%;padding:10rpx 0;height:150rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between}.main .info .left, .main .info .right{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-flow:column;flex-flow:column;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center}.main .info .left{font-size:24rpx}.main .info .left view{padding:5rpx 0}.main .info .right{-webkit-box-align:center;-webkit-align-items:center;align-items:center;font-size:34rpx;color:#eead3c;font-weight:550}.main .info .right view{padding:7rpx 0}