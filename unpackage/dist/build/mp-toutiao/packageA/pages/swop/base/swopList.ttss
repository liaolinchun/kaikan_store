.guess-section{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap;padding:0 15rpx;background:#f2f2f2;position:relative;top:200rpx}.guess-section .guess-item{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;width:49%;padding-bottom:20rpx;box-sizing:border-box;overflow:hidden}.guess-section .guess-item:nth-child(2n+1){margin-right:2%}.guess-section .image-wrapper{width:100%;height:330rpx;border-radius:0;overflow:hidden}.guess-section .image-wrapper image{width:100%;height:100%;opacity:1}.guess-section .fontTitle{width:100%;font-size:24rpx;line-height:30rpx;height:60rpx;background-color:#fff;display:-webkit-box;overflow:hidden;text-overflow:ellipsis;word-wrap:break-word;white-space:normal !important;-webkit-line-clamp:2;-webkit-box-orient:vertical}.guess-section .price{width:100%;height:40rpx;display:-webkit-box;display:-webkit-flex;display:flex;background-color:#fff;padding:20rpx 15rpx}.guess-section .price .old{-webkit-box-flex:1;-webkit-flex:1;flex:1;font-size:26rpx;padding:0 15rpx 0 5rpx;color:#ff6783;font-weight:600;line-height:40rpx}.guess-section .price .new{-webkit-box-flex:1;-webkit-flex:1;flex:1;font-size:26rpx;padding:0 15rpx 0 5rpx;color:#333;text-align:center;line-height:40rpx;font-weight:500;background-color:#fdeb46}.guess-section .integral{width:100%;height:50rpx;background-color:#fdeb46;font-size:18rpx;line-height:50rpx;text-align:center}