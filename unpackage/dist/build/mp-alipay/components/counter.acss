.number{height:50rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center}.number .box{display:-webkit-box;display:-webkit-flex;display:flex;border:1px solid #eee}.number .inputs{width:60rpx;height:40rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;text-align:center}.number .inputs input{width:60rpx;height:40rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;text-align:center;font-size:26rpx}.number .title{font-size:28rpx;color:#999;padding-right:18rpx}.number .minus, .number .add{width:40rpx;height:40rpx;border-radius:5rpx}.number .minus .icon, .number .add .icon{line-height:40rpx;font-size:38rpx;color:#999;width:40rpx;height:40rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center}