<view><pay-header vue-id="0e318f9f-1" goodsInfo="{{goodsInfo}}" addressData="{{addressData}}" onVueInit="__l"></pay-header><view class="goods-section"><view class="g-header b-b"><text class="name">{{goodsInfo.detail[0].mer_name}}</text></view><view class="g-item"><image src="{{'http://www.tiktokshopee.com'+goodsInfo.detail[0].thumbimg}}"></image><view class="right"><view class="title-box"><view class="on">商城</view><text class="title clamp">{{goodsInfo.detail[0].title}}</text></view><view class="spec-box"><text class="spec">规格:</text><text class="number">{{goodsInfo.detail[0].size}}</text></view><view class="price-box"><text class="price">{{"￥"+goodsInfo.all_price}}</text><text class="price-old">{{"￥"+goodsInfo.all_price}}</text><view class="number">{{"x"+goodsInfo.detail[0].num}}</view></view></view></view><view class="fee"><text>配送费</text><text>￥0.00</text></view></view><view style="height:170rpx;"></view><block a:if="{{!showPay}}"><view class="footer"><view class="price-content"><view class="price-tip">共3件</view><view><text class="price-tip">合计:</text><text class="price">{{"￥"+goodsInfo.detail[0].num*goodsInfo.all_price}}</text></view></view><text data-event-opts="{{[['tap',[['toPay',['$event']]]]]}}" class="submit" onTap="__e">去支付</text></view></block><popup-pay style="z-index:9999;" vue-id="0e318f9f-2" showPay="{{showPay}}" Price="{{sum}}" ref="__r" data-ref="Series" data-event-opts="{{[['^func',[['setPay']]]]}}" onFunc="__e" onVueInit="__l"></popup-pay></view>