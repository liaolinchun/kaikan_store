<view><block a:for="{{goodsInfo}}" a:for-item="item" a:for-index="index" a:key="index"><view class="box"><view class="time">{{"结束时间 "+item.time}}</view><view class="main"><view class="left"><image src="{{item.img}}"></image></view><view class="center"><view class="top">item.title</view><view class="btn"><text class="new">{{"￥"+item.price}}</text><text class="old">￥1200.00</text></view></view><view class="right"><view>好友贡献</view><view class="cur">0.47</view></view></view><view class="footer"><view></view><view class="btn"><view data-event-opts="{{[['tap',[['share',['$event']]]]]}}" class="type" onTap="__e">分享帮砍</view><view data-event-opts="{{[['tap',[['toOrder',['$0'],[[['goodsInfo','',index]]]]]]]}}" class="type cur" onTap="__e">去下单</view></view></view></view></block><block a:if="{{wechatShow}}"><wechat vue-id="27c9bf3a-1" goodsInfo="{{goodsInfo}}" money="{{money}}" ref="__r" data-ref="Series" onVueInit="__l"></wechat></block><block a:if="{{loading}}"><view class="loading-text">{{loadingText}}</view></block></view>