<view hidden="{{!(showSite)}}" class="popup site"><view class="mask"></view><view class="layer"><view class="site-header"><text class="font">配送至</text><text data-event-opts="{{[['tap',[['hideSpec',['$event']]]]]}}" class="icon iconfont" catchTap="__e"></text></view><view class="nav"><block a:for="{{siteNav}}" a:for-item="name" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['showSiteNav',[index]]]]]}}" class="{{(('type')+' '+((index==showSiteIndex)?'cur':''))}}" onTap="__e">{{name}}</view></block></view><scroll-view class="city" scroll-y="true"><block a:for="{{province}}" a:for-item="prov" a:for-index="index1" a:key="index1"><block a:if="{{showOne}}"><view data-event-opts="{{[['tap',[['showProvNav',['$0',index1],[[['province','',index1]]]]]]]}}" class="{{(('row')+' '+((index1==showProvIndex)?'on':''))}}" onTap="__e">{{''+prov+''}}</view></block></block><block a:for="{{citys}}" a:for-item="city" a:for-index="index2" a:key="index2"><block a:if="{{showTwo}}"><view data-event-opts="{{[['tap',[['showCityNav',['$0',index2],[[['citys','',index2]]]]]]]}}" class="{{(('row')+' '+((index2==showCityIndex)?'on':''))}}" onTap="__e">{{''+city+''}}</view></block></block><block a:for="{{region}}" a:for-item="reg" a:for-index="index3" a:key="index3"><block a:if="{{showThree}}"><view data-event-opts="{{[['tap',[['overSelect',['$0',index3],[[['region','',index3]]]]]]]}}" class="{{(('row')+' '+((index3==showRegIndex)?'on':''))}}" onTap="__e">{{''+reg+''}}</view></block></block></scroll-view></view></view>