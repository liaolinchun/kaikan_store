<view class="content"><block a:for="{{addressList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{addressList.length!=0}}"><view data-event-opts="{{[['tap',[['checkAddress',['$0'],[[['addressList','',index]]]]]]]}}" class="list" onTap="__e"><view class="wrapper"><view class="address-box"><block a:if="{{item.mo_ren==1}}"><text class="tag">默认</text></block><text class="address">{{item.province+" "+item.area}}</text></view><view class="u-box"><text class="name">{{item.shou_name}}</text><text class="mobile">{{item.shou_tell}}</text></view></view><text data-event-opts="{{[['tap',[['delAddress',['$0'],[[['addressList','',index]]]]]]]}}" class="icon-bianji iconfont" style="color:#999;" catchTap="__e"></text><text data-event-opts="{{[['tap',[['addAddress',['edit','$0'],[[['addressList','',index]]]]]]]}}" class="icon-bianji iconfont" catchTap="__e"></text></view></block></block><block a:if="{{addressList.length==0}}"><text class="cur">请添加收货地址</text></block><button data-event-opts="{{[['tap',[['addAddress',['add']]]]]}}" class="add-btn" onTap="__e">新增地址</button></view>