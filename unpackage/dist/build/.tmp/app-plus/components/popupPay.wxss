.popup {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 20;
}
.popup .mask {
    position: fixed;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 21%;
    background-color: rgba(0, 0, 0, 0.6);
}
.popup .layer {
    position: fixed;
    z-index: 22;
    bottom: 0;
    width: 100%;
    min-height: 15%;
    background-color: #fff;
    -webkit-align-content: space-between;
            align-content: space-between;
}
.popup .layer .pay-title {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      width: 96%;
      padding: 0 2%;
      padding: 10rpx 0;
      color: #666;
      font-size: 32rpx;
}
.popup .layer .pay-title .title {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
                flex: 1;
        text-align: center;
        font-size: 38rpx;
        padding-left: 80rpx;
}
.popup .layer .money {
      color: #FFCC66;
      width: 100%;
      font-size: 60rpx;
      font-weight: 600;
      text-align: center;
}
.popup .layer .pay-list {
      width: 94%;
      padding: 0 3%;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: vertical;
      -webkit-box-direction: normal;
      -webkit-flex-direction: column;
              flex-direction: column;
}
.popup .layer .pay-list .mode {
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: justify;
        -webkit-justify-content: space-between;
                justify-content: space-between;
        width: 100%;
        height: 120rpx;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
}
.popup .layer .pay-list .mode .icon {
          display: -webkit-box;
          display: -webkit-flex;
          display: flex;
          -webkit-box-align: center;
          -webkit-align-items: center;
                  align-items: center;
}
.popup .layer .pay-list .mode image {
          width: 100rpx;
          height: 100rpx;
          z-index: 9999;
}
.popup .layer .pay-list .mode .font {
          color: #666;
          font-size: 36rpx;
          padding-left: 20rpx;
}
.popup .layer .pay-list .mode .sel {
          width: 40rpx;
          height: 40rpx;
          border: 1px solid #ddd;
          border-radius: 50%;
          margin-right: 5rpx;
}
.popup .layer .pay-list .mode .sel.cur {
            background-size: 100% 100%;
            border: 1px solid #40BB0A;
            background-image: url(data:image/png;base64,);
}
.popup .layer .deal {
      width: 94%;
      padding: 0 3%;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      height: 50rpx;
      margin-top: 40rpx;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.popup .layer .deal .box1 {
        width: 5vw;
        height: 5vw;
        border: 1px #ddd solid;
}
.popup .layer .deal .box1.cur {
          background-image: url(data:image/png;base64,);
          background-size: 100% 100%;
}
.popup .layer .deal .box2 {
        font-size: 28rpx;
        margin-left: 20rpx;
        color: #333;
}
.popup .btn {
    margin-top: 20rpx;
    width: 100%;
    height: 120rpx;
    z-index: 999;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    color: #fff;
    background-color: red;
}

