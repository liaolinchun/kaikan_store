<view><video hidden="{{!(showVideo)}}" class="myVideo" id="myVideo" src="{{videoSrc}}" show-fullscreen-btn="{{showFullscreenBtn}}" direction="{{videoDirection}}" data-event-opts="{{[['pause',[['videoPause',['$event']]]],['fullscreenchange',[['viderFullscreen',['$event']]]]]}}" onPause="__e" onFullscreenchange="__e"></video><block a:if="{{this.showNull}}"><view class="null">暂无评论~</view></block><view class="content"><view class="list"><block a:for="{{ratingsList}}" a:for-item="row" a:for-index="Rindex" a:key="Rindex"><view class="row"><view class="left"><view class="face"><image src="{{row.face}}"></image></view></view><view class="right"><view class="name-date"><view class="username">{{''+row.username+''}}</view><view class="star"><block a:for="{{clicked_list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="iconfont"><block a:if="{{item}}"><view></view></block><block a:else><view class="starIcon"></view></block></view></block></view></view><view class="spec"><view class="date">{{''+row.time+''}}</view><view>{{''+row.spec+''}}</view></view><view class="first"><view class="rat">{{''+row.content+''}}</view><view class="img-video"><block a:for="{{row.video}}" a:for-item="item" a:for-index="__i0__" a:key="path"><view data-event-opts="{{[['tap',[['playVideo',['$0'],[[['ratingsList','',Rindex],['video','path',item.path,'path']]]]]]]}}" class="box" onTap="__e"><image mode="aspectFill" src="{{item.img}}"></image><view class="playbtn"><view class="icon iconfont"></view></view></view></block><block a:for="{{row.img}}" a:for-item="item" a:for-index="__i1__" a:key="*this"><view data-event-opts="{{[['tap',[['showBigImg',['$0','$1'],[[['ratingsList','',Rindex],['img','',__i1__]],[['ratingsList','',Rindex,'img']]]]]]]}}" class="box" onTap="__e"><image mode="aspectFill" src="{{item}}"></image></view></block></view></view></view></view></block></view></view></view>