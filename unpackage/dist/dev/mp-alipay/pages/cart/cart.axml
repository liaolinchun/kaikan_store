<view><block a:if="{{!hasLogin||goodsList.length==0}}"><view class="empty"><image src="/static/emptyCart.jpg" mode="aspectFit"></image><block a:if="{{hasLogin}}"><view class="empty-tips">空空如也<block a:if="{{hasLogin}}"><navigator class="navigator" url="../home/home" open-type="switchTab">随便逛逛></navigator></block></view></block><block a:else><view class="empty-tips">还没登录<view data-event-opts="{{[['tap',[['navToLogin',['$event']]]]]}}" class="navigator" onTap="__e">去登陆></view></view></block></view></block><view class="goods-list"><block a:for="{{goodsList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="row"><view data-event-opts="{{[['tap',[['handleSingleDelete',['$0'],[[['goodsList','',index]]]]]]]}}" class="menu" onTap="__e"><view class="icon iconfont"></view></view><view data-event-opts="{{[['touchStart',[['handleTouchStart',[index,'$event']]]],['touchMove',[['handleTouchMove',[index,'$event']]]],['touchEnd',[['handleTouchEnd',[index,'$event']]]]]}}" class="{{(('production')+' '+(theIndex==index?'open':oldIndex==index?'close':''))}}" onTouchStart="__e" onTouchMove="__e" onTouchEnd="__e"><view data-event-opts="{{[['tap',[['handleCheckbox',['$0'],[[['goodsList','',index]]]]]]]}}" class="container" onTap="__e"><view class="checkbox"><view class="{{((item.selected)?'on':'')}}"></view></view></view><view data-event-opts="{{[['tap',[['handleGoodsInfo',['$0'],[[['goodsList','',index]]]]]]]}}" class="goods-info" onTap="__e"><view class="img"><image src="{{'http://www.tiktokshopee.com'+item.thumbimg}}"></image></view><view class="info"><view class="title">{{item.title}}</view><view class="spec">{{item.color_name}}</view><view class="price-number"><view class="price">{{item.price_one}}</view><counter vue-id="{{'176bf896-1-'+index}}" goodsInfo="{{item}}" data-event-opts="{{[['^add',[['add',['$0'],[[['goodsList','',index]]]]]],['^minus',[['minus',['$0'],[[['goodsList','',index]]]]]]]}}" onAdd="__e" onMinus="__e" onVueInit="__l"></counter></view></view></view></view></view></block></view><block a:if="{{!goodsList.length==0}}"><view class="footer" style="{{'bottom:'+(footerbottom)+';'}}"><view class="sum_price"><view class="check-box"><view data-event-opts="{{[['tap',[['handleSelectedAll',['$event']]]]]}}" class="checkbox" onTap="__e"><view class="{{((isAllSelected)?'on':'')}}"></view></view><block a:if="{{selectedList.length>0}}"><view data-event-opts="{{[['tap',[['handleMulDelete',['$event']]]]]}}" class="{{(('del')+' '+((isAllSelected)?'show':''))}}" onTap="__e">清空</view></block></view><view><text>合计 :</text><text class="price">{{'￥'+sumPrice}}</text></view></view><view data-event-opts="{{[['tap',[['toPay',['$event']]]]]}}" class="{{(('btn')+' '+((sumPrice!='0.00')?'on':''))}}" onTap="__e">结算</view></view></block><popup-pay vue-id="176bf896-2" showPay="{{showPay}}" Price="{{sumPrice}}" footerbottom="{{footerbottom}}" data-event-opts="{{[['^func',[['setPay']]]]}}" onFunc="__e" onVueInit="__l"></popup-pay></view>