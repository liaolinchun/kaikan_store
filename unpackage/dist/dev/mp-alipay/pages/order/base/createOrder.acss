page {
  background: #F4f4f4;
  padding-bottom: 100rpx;
}
.address-section {
  padding: 30rpx 0;
  background: #fff;
  position: relative;
}
.address-section .order-content {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.address-section .icon-shouhuodizhi {
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    width: 90rpx;
    color: #888;
    font-size: 44rpx;
}
.address-section .cen {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    font-size: 28rpx;
    color: #999;
    padding-left: 30rpx;
}
.address-section .cen .top {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      position: relative;
}
.address-section .cen .top .default {
        padding: 0 5rpx;
        margin-left: 3vw;
        background-color: #E4E4E4;
        border-radius: 20rpx;
        color: #FFFFFF;
}
.address-section .cen .top .alter {
        position: absolute;
        right: 3vw;
        background-color: #EE666C;
        color: #FFFFFF;
        height: 5vw;
        line-height: 5vw;
        padding: 0 10rpx;
        border-radius: 20rpx;
}
.address-section .name {
    font-size: 34rpx;
    margin-right: 24rpx;
}
.address-section .address {
    margin-top: 16rpx;
    margin-right: 20rpx;
    color: #999;
}
.address-section .icon-you {
    font-size: 32rpx;
    color: #999;
    margin-right: 30rpx;
}
.address-section .a-bg {
    position: absolute;
    left: 0;
    bottom: 0;
    display: block;
    width: 100%;
    height: 5rpx;
}
.goods-section {
  margin-top: 16rpx;
  background: #fff;
  padding-bottom: 1px;
}
.goods-section .g-header {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    height: 84rpx;
    padding: 0 30rpx;
    position: relative;
    border-bottom: 1px solid #eee;
}
.goods-section .logo {
    display: block;
    width: 50rpx;
    height: 50rpx;
    border-radius: 100px;
    background-color: #990000;
}
.goods-section .name {
    font-size: 34rpx;
    color: #000;
    margin-left: 5rpx;
}
.goods-section .order_number {
    font-size: 28rpx;
    color: #999;
    padding-left: 20rpx;
}
.goods-section .g-item {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    margin: 20rpx 30rpx;
}
.goods-section .g-item image {
      -webkit-flex-shrink: 0;
              flex-shrink: 0;
      display: block;
      width: 170rpx;
      height: 180rpx;
      border-radius: 4rpx;
}
.goods-section .g-item .right {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
              flex: 1;
      padding-left: 24rpx;
      overflow: hidden;
}
.goods-section .g-item .right .title-box {
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
}
.goods-section .g-item .right .title-box .on {
          width: 8vw;
          height: 5vw;
          text-align: center;
          line-height: 5vw;
          color: #FF9900;
          background-color: #FEEDED;
          font-size: 24rpx;
          border: 1px solid #FF9900;
}
.goods-section .g-item .right .title-box .off {
          width: 8vw;
          height: 5vw;
          text-align: center;
          line-height: 5vw;
          color: #DA5C4E;
          background-color: #FEEDED;
          font-size: 24rpx;
          border: 1px solid #DA5C4E;
}
.goods-section .g-item .right .title-box .title {
          -webkit-box-flex: 1;
          -webkit-flex: 1;
                  flex: 1;
          font-size: 32rpx;
          color: #333;
          padding-left: 20rpx;
          display: -webkit-box;
          overflow: hidden;
          text-overflow: ellipsis;
          word-wrap: break-word;
          white-space: normal !important;
          -webkit-line-clamp: 2;
          -webkit-box-orient: vertical;
}
.goods-section .g-item .right .title-box .spec {
          font-size: 26rpx;
          color: #999;
}
.goods-section .g-item .spec-box {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      font-size: 32rpx;
      padding: 3rpx 0;
      color: #999;
}
.goods-section .g-item .spec-box .spec {
        margin-bottom: 4rpx;
}
.goods-section .g-item .spec-box .number {
        font-size: 26rpx;
        color: #999;
        margin-left: 20rpx;
}
.goods-section .g-item .price-box {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      font-size: 32rpx;
      color: #999;
      position: relative;
}
.goods-section .g-item .price-box .price {
        color: red;
        font-size: 38rpx;
        margin-bottom: 4rpx;
}
.goods-section .g-item .price-box .price-old {
        padding-left: 25rpx;
        font-size: 28rpx;
        text-decoration: line-through;
}
.goods-section .g-item .price-box .number {
        font-size: 36rpx;
        color: #333;
        margin-left: 20rpx;
        position: absolute;
        right: 0;
}
.goods-section .g-item .step-box {
      position: relative;
}
.goods-section .fee {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    font-size: 32rpx;
    color: #666;
    margin: 5rpx 30rpx;
}
/*底部*/
.footer {
  position: fixed;
  left: 0;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  width: 100%;
  height: 180rpx;
  font-size: 30rpx;
  background-color: #fff;
  z-index: 998;
  color: red;
  box-shadow: 0 -1px 5px rgba(0, 0, 0, 0.1);
}
.footer .price-content {
    width: 92%;
    margin: 15rpx 4% 5rpx 4%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.footer .price-content .price-tip {
      color: #333;
      font-size: 34rpx;
      padding-right: 15rpx;
}
.footer .price-content .price {
      font-size: 32rpx;
      color: red;
}
.footer .submit {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    width: 92%;
    height: 80rpx;
    margin: 10rpx 4%;
    color: #fff;
    font-size: 32rpx;
    background-color: #EE666C;
}

