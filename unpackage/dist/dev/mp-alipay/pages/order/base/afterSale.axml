<view><view class="order_number">订单号 : 8545548461545</view><view class="inp_list"><view><view class="type">退货原因</view><input type="text" placeholder="请填写退货原因" data-event-opts="{{[['input',[['__set_model',['','cause_val','$event',[]]]]]]}}" value="{{cause_val}}" onInput="__e"/></view><view><view class="type">联系方式</view><input type="text" placeholder="输入联系方式" data-event-opts="{{[['input',[['__set_model',['','phone_val','$event',[]]]]]]}}" value="{{phone_val}}" onInput="__e"/></view><view><view class="type">申请原因</view><input type="text" placeholder="请输入申请原因" data-event-opts="{{[['input',[['__set_model',['','apply_val','$event',[]]]]]]}}" value="{{apply_val}}" onInput="__e"/></view></view><block a:if="{{imgList.length!=3}}"><view data-event-opts="{{[['tap',[['setPhoto',['$event']]]]]}}" class="photo" onTap="__e"><view class="icon iconfont"></view><view class="icon_tit">上传凭证 最多三张</view></view></block><block a:if="{{imgList.length>0}}"><view class="show_img"><block a:for="{{imgList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="img_box" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" onTap="__e"><image src="{{imgList[index]}}"></image><view class="cu-tag" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchTap="__e"><text class="iconfont" style="font-size:46rpx;"></text></view></view></block></view></block><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn" onTap="__e">提交申请</view></view>