page {
  background-color: #f2f2f2;
}
.box {
  width: 100%;
  position: relative;
}
.box .bg_top {
    width: 100%;
    height: 180rpx;
    background-color: #FFCC33;
}
.box .bg_bot {
    width: 100%;
    height: 360rpx;
    background-color: #66FFCC;
}
.box .bg_bot .title {
      width: 92%;
      font-size: 30rpx;
      color: #333;
      font-weight: 550;
      padding: 130rpx 4% 20rpx;
}
.box .bg_bot .card {
      width: 92%;
      height: 160rpx;
      background-color: #E6212A;
      margin: 0 4%;
      z-index: 99;
      border-radius: 40rpx;
      position: absolute;
      bottom: 20rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
}
.box .bg_bot .card .icon {
        font-size: 48rpx;
        color: #fff;
        padding: 30rpx;
}
.box .bg_bot .card .font_cart {
        border: 1px solid red;
        padding-top: 15rpx;
}
.box .bg_bot .card .font_cart .font_o {
          color: #fff;
          font-size: 54rpx;
}
.box .bg_bot .card .font_cart .font_t {
          font-size: 32rpx;
          color: #ccc;
}
.box .bg_bot .card .click_icon {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
                flex: 1;
        position: relative;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
        overflow: hidden;
}
.box .bg_bot .card .click_icon .box {
          position: absolute;
          opacity: .4;
}
.box .bg_bot .card .click_icon .img {
          width: 80rpx;
          height: 80rpx;
          position: absolute;
          right: 90rpx;
          background-image: url(data:image/png;base64,);
          background-size: 100% 100%;
}
.box .bg_bot .card .click_icon .box1 {
          width: 80rpx;
          height: 80rpx;
          border-radius: 50%;
          background-color: #fff;
          right: 90rpx;
}
.box .bg_bot .card .click_icon .box2 {
          width: 150rpx;
          height: 150rpx;
          border-radius: 50%;
          border: 25rpx #fff solid;
          right: 35rpx;
}
.box .bg_bot .card .click_icon .box3 {
          width: 300rpx;
          height: 300rpx;
          border-radius: 50%;
          border: 25rpx #fff solid;
          right: -55rpx;
}
.box .clubCard {
    width: 70%;
    height: 260rpx;
    border-radius: 30rpx;
    border: 7rpx solid #fff;
    position: absolute;
    top: 20rpx;
    z-index: 99;
    left: 50%;
    -webkit-transform: translateX(-50%);
            transform: translateX(-50%);
    background-color: #C02126;
}
.box .clubCard .price {
      width: 50%;
      height: 50rpx;
      border-radius: 50rpx;
      background-color: #FFCC00;
      position: absolute;
      bottom: 55rpx;
      left: 50%;
      text-align: center;
      font-size: 26rpx;
      line-height: 50rpx;
      -webkit-transform: translateX(-50%);
              transform: translateX(-50%);
}
