/*<!-- 轮播图-->*/
.swiper-box {
  position: relative;
  width: 100%;
  height: 85vw;
}
.swiper-box swiper {
    width: 100%;
    height: 90vw;
}
.swiper-box swiper swiper-item image {
      width: 100%;
      height: 100%;
}
.swiper-box .indicator {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    padding: 0 25rpx;
    height: 40rpx;
    border-radius: 40rpx;
    font-size: 22rpx;
    position: absolute;
    bottom: 20rpx;
    right: 20rpx;
    color: #fff;
    background-color: rgba(0, 0, 0, 0.2);
}
.time {
  width: 100%;
  height: 75rpx;
  background-color: #FF3333;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
}
.time .time_icon {
    height: 92%;
    padding: 0 4%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    color: #FFFFFF;
}
.time .time_icon image {
      width: 50rpx;
      height: 50rpx;
      border-radius: 50%;
}
.time .time_run {
    color: #FFFFFF;
    text-align: center;
    font-size: 26rpx;
    position: absolute;
    left: 50%;
    -webkit-transform: translateX(-50%);
            transform: translateX(-50%);
}
.time .time_run .run {
      font-size: 32rpx;
      font-weight: 550;
      padding: 0 8rpx;
}
