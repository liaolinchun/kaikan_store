<view><block wx:if="{{!hasLogin||goodsList.length==0}}"><view class="empty"><image src="/static/emptyCart.jpg" mode="aspectFit"></image><block wx:if="{{hasLogin}}"><view class="empty-tips">空空如也<block wx:if="{{hasLogin}}"><navigator class="navigator" url="../home/home" open-type="switchTab">随便逛逛></navigator></block></view></block><block wx:else><view class="empty-tips">还没登录<view data-event-opts="{{[['tap',[['navToLogin',['$event']]]]]}}" class="navigator" bindtap="__e">去登陆></view></view></block></view></block><view class="goods-list"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="row"><view data-event-opts="{{[['tap',[['handleSingleDelete',['$0'],[[['goodsList','',index]]]]]]]}}" class="menu" bindtap="__e"><view class="icon iconfont"></view></view><view data-event-opts="{{[['touchstart',[['handleTouchStart',[index,'$event']]]],['touchmove',[['handleTouchMove',[index,'$event']]]],['touchend',[['handleTouchEnd',[index,'$event']]]]]}}" class="{{['production',theIndex==index?'open':oldIndex==index?'close':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['handleCheckbox',['$0'],[[['goodsList','',index]]]]]]]}}" class="container" bindtap="__e"><view class="checkbox"><view class="{{[(item.selected)?'on':'']}}"></view></view></view><view data-event-opts="{{[['tap',[['handleGoodsInfo',['$0'],[[['goodsList','',index]]]]]]]}}" class="goods-info" bindtap="__e"><view class="img"><image src="{{'http://www.tiktokshopee.com'+item.thumbimg}}"></image></view><view class="info"><view class="title">{{item.title}}</view><view class="spec">{{item.color_name}}</view><view class="price-number"><view class="price">{{item.price_one}}</view><counter vue-id="{{'1775f8c0-1-'+index}}" goodsInfo="{{item}}" data-event-opts="{{[['^add',[['add',['$0'],[[['goodsList','',index]]]]]],['^minus',[['minus',['$0'],[[['goodsList','',index]]]]]]]}}" bind:add="__e" bind:minus="__e" bind:__l="__l"></counter></view></view></view></view></view></block></view><block wx:if="{{!goodsList.length==0}}"><view class="footer" style="{{'bottom:'+(footerbottom)+';'}}"><view class="sum_price"><view class="check-box"><view data-event-opts="{{[['tap',[['handleSelectedAll',['$event']]]]]}}" class="checkbox" bindtap="__e"><view class="{{[(isAllSelected)?'on':'']}}"></view></view><block wx:if="{{selectedList.length>0}}"><view data-event-opts="{{[['tap',[['handleMulDelete',['$event']]]]]}}" class="{{['del',(isAllSelected)?'show':'']}}" bindtap="__e">清空</view></block></view><view><text>合计 :</text><text class="price">{{'￥'+sumPrice}}</text></view></view><view data-event-opts="{{[['tap',[['toPay',['$event']]]]]}}" class="{{['btn',(sumPrice!='0.00')?'on':'']}}" bindtap="__e">结算</view></view></block><popup-pay vue-id="1775f8c0-2" showPay="{{showPay}}" Price="{{sumPrice}}" footerbottom="{{footerbottom}}" data-event-opts="{{[['^func',[['setPay']]]]}}" bind:func="__e" bind:__l="__l"></popup-pay></view>