<view class="content"><page-status vue-id="0e0a4db1-1" bind:__l="__l"></page-status><view class="row b-b"><text class="tit">收件人</text><input class="input" type="text" disabled="{{key}}" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','shou_name','$event',[]],['addressData']]]]]}}" value="{{addressData.shou_name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">联系手机</text><input class="input" type="number" disabled="{{key}}" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','shou_tell','$event',[]],['addressData']]]]]}}" value="{{addressData.shou_tell}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['setShowSite',['$event']]]]]}}" class="row b-b" bindtap="__e"><text class="tit">所在地区</text><text class="input">{{''+addressData.addressName+''}}</text><text class="icon-shouhuodizhi iconfont"></text></view><view class="row b-b"><text class="tit">详细地址</text><input class="input" type="text" disabled="{{key}}" placeholder="街道、门牌号等" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['addressData']]]]]}}" value="{{addressData.address}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设置默认地址</text><switch checked="{{addressData.showCheck}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button><popup-site class="vue-ref" vue-id="0e0a4db1-2" showSite="{{showSite}}" data-ref="getCity" data-event-opts="{{[['^func',[['setSelect']]]]}}" bind:func="__e" bind:__l="__l"></popup-site></view>